{
  "env": {
    "browser": true,
    "es2020": true,
    "node": true,
    "jest": true
  },
  "extends": [
    "plugin:react/recommended",
    "prettier/@typescript-eslint",
    "prettier/react",
    "airbnb",
    "prettier"
  ],
  "plugins": ["prettier"],
  "rules": {
    "prettier/prettier": ["error"],
    "space-before-function-paren": "off",
    "react/prop-types": "off"
  }
}
